{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "preserve",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"allowJs": true,
		"lib": ["dom", "dom.iterable", "esnext"]
	}
}
